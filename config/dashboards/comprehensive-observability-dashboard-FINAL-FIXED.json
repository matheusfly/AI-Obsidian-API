{
  "id": null,
  "uid": "comprehensive-observability-final-fixed",
  "title": "Data Vault Obsidian - Comprehensive Observability (FINAL FIXED)",
  "tags": ["data-vault", "obsidian", "observability", "comprehensive", "final-fixed"],
  "style": "dark",
  "timezone": "browser",
  "refresh": "5s",
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "title": "System Health Overview",
      "type": "stat",
      "targets": [
        {
          "expr": "up{job=\"data-pipeline\"}",
          "legendFormat": "Data Pipeline"
        },
        {
          "expr": "up{job=\"prometheus\"}",
          "legendFormat": "Prometheus"
        },
        {
          "expr": "up{job=\"otel-collector\"}",
          "legendFormat": "OTEL Collector"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "red",
                  "text": "Down"
                },
                "1": {
                  "color": "green",
                  "text": "Up"
                }
              },
              "type": "value"
            }
          ]
        }
      },
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Total HTTP Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(http_requests_total{job=\"data-pipeline\"})",
          "legendFormat": "Total Requests"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 3,
      "title": "HTTP Request Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(http_requests_total{job=\"data-pipeline\"}[5m])",
          "legendFormat": "Requests/sec"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 4,
      "title": "Response Time P95",
      "type": "stat",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"data-pipeline\"}[5m]))",
          "legendFormat": "P95 Response Time"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          },
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 5,
      "title": "ChromaDB Collection Size",
      "type": "stat",
      "targets": [
        {
          "expr": "chroma_collection_size{job=\"data-pipeline\"}",
          "legendFormat": "Collection Size"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "palette-classic"
          }
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
    },
    {
      "id": 6,
      "title": "ChromaDB Query Rate",
      "type": "stat",
      "targets": [
        {
          "expr": "rate(chromadb_query_total{job=\"data-pipeline\"}[5m])",
          "legendFormat": "Queries/sec"
